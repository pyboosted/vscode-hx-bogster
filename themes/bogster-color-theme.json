    "inputValidation.errorBorder": "#ee4d7f",
    
    "editorOverviewRuler.errorForeground": "#ee4d7f",
    "editorOverviewRuler.warningForeground": "#dc7759",
    "editorOverviewRuler.infoForeground": "#00DFB5",
    "editorOverviewRuler.modifiedForeground": "#dcb659",
    
    "problemsErrorIcon.foreground": "#ee4d7f",
    "problemsWarningIcon.foreground": "#dc7759",
    "problemsInfoIcon.foreground": "#00DFB5",
    
    "editorUnnecessaryCode.opacity": "#00000066",
    "editorUnnecessaryCode.border": "#00000000",
    
    "debugConsole.errorForeground": "#ee4d7f",
    "debugConsole.warningForeground": "#dc7759",
    "debugConsole.infoForeground": "#00DFB5",
    
    "notificationsErrorIcon.foreground": "#ee4d7f",
    "notificationsWarningIcon.foreground": "#dc7759",
    "notificationsInfoIcon.foreground": "#00DFB5",
    
    "editorGutter.addedBackground": "#00DFB5",
    "editorGutter.modifiedBackground": "#dcb659",
    "editorGutter.deletedBackground": "#ee4d7f",
    
    "diffEditor.insertedTextBackground": "#00DFB533",
    "diffEditor.removedTextBackground": "#ee4d7f33",
    
    "gitDecoration.modifiedResourceForeground": "#dcb659",
    "gitDecoration.deletedResourceForeground": "#ee4d7f",
    "gitDecoration.untrackedResourceForeground": "#00DFB5",
    "gitDecoration.ignoredResourceForeground": "#415367",
    "gitDecoration.conflictingResourceForeground": "#b759dc",
    
    "editorWidget.background": "#232d38",
    "editorWidget.foreground": "#e5ded6",

    "terminal.ansiBrightBlue": "#59dcd8",
    "terminal.ansiBrightMagenta": "#b759dc",
    "terminal.ansiBrightCyan": "#59dcd8",
    "terminal.ansiBrightWhite": "#e5ded6",
    
    "editorError.foreground": "#ee4d7f",
    "editorWarning.foreground": "#dc7759",
    "editorInfo.foreground": "#00DFB5",
    "editorHint.foreground": "#36b2d4",

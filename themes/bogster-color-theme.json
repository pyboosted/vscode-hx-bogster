    "scrollbar.shadow": "#00000030",
    "widget.shadow": "#00000030",
    "editorGroup.dropBackground": "#313f4e30"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable.declaration": "#e5ded6",
    "variable.declaration.readonly": "#e5ded6",
    "property.declaration": "#e5ded6",
    "parameter.declaration": "#c6b8ad"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#00DFB5"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#7fdc59",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#36b2d4"
      }
    },
    {
      "name": "Built-in constant",
      "scope": ["constant.language", "constant.builtin"],
      "settings": {
        "foreground": "#00DFB5"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#00DFB5"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#c6b8ad"
      }
    },
    {
      "name": "Variable Declaration",
      "scope": [
        "variable.other.constant",
        "variable.other.readwrite",
        "meta.definition.variable",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#e5ded6"
      }
    },
    {
      "name": "Member Variable",
      "scope": ["variable.member", "variable.other.member"],
      "settings": {
        "foreground": "#c6b8ad"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "keyword.control.import", "keyword.control.from"],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword Directive",
      "scope": ["keyword.directive", "keyword.other.directive"],
      "settings": {
        "foreground": "#dcb659"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#dc7759",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "punctuation.delimiter"],
      "settings": {
        "foreground": "#dc7759"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#59dcd8"
      }
    },
    {
      "name": "Built-in Function",
      "scope": ["support.function", "support.function.builtin"],
      "settings": {
        "foreground": "#59dcd8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function Macro",
      "scope": ["entity.name.function.macro"],
      "settings": {
        "foreground": "#dc7759",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class, Type",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#dc597f"
      }
    },
    {
      "name": "Built-in Type",
      "scope": ["support.type.builtin", "support.type.primitive"],
      "settings": {
        "foreground": "#d32c5d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constructor",
      "scope": ["entity.name.function.constructor", "meta.constructor"],
      "settings": {
        "foreground": "#dc597f"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#dc7759"
      }
    },
    {
      "name": "Namespace",
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#d32c5d"
      }
    },
    {
      "name": "Special",
      "scope": ["keyword.other.special-method"],
      "settings": {
        "foreground": "#7fdc59"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.control.default",
        "keyword.control.type",
        "keyword.import",
        "keyword.export"
      ],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage Type",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Type Keyword",
      "scope": [
        "keyword.type",
        "keyword.other.type",
        "storage.type.type",
        "storage.type.interface",
        "storage.type.enum"
      ],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "TypeScript specific",
      "scope": [
        "entity.name.type.module",
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Label",
      "scope": ["entity.name.label"],
      "settings": {
        "foreground": "#36b2d4"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag"],
      "settings": {
        "foreground": "#d32c5d"
      }
    },
    {
      "name": "Tag Attribute",
      "scope": ["meta.tag entity.other.attribute-name"],
      "settings": {
        "foreground": "#dc7759"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ee4d7f"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#36b2d4"
      }
    },
    {
      "name": "Markup List",
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#e5ded6"
      }
    },
    {
      "name": "Markdown Text",
      "scope": [
        "text.html.markdown",
        "source.gfm",
        "text.html.markdown.gfm"
      ],
      "settings": {
        "foreground": "#e5ded6"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.list.markdown",
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#b759dc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#dcb659",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Link Text",
      "scope": ["string.other.link"],
      "settings": {
        "foreground": "#d32c5d"
      }
    },
    {
      "name": "Markup Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#00DFB5"
      }
    },
    {
      "name": "Markup Raw",
      "scope": ["markup.raw", "markup.inline.raw"],
      "settings": {
        "foreground": "#7fdc59"
      }
    },
    {
      "name": "Diff Added",
      "scope": ["markup.inserted", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#00DFB5"
      }
    },
    {
      "name": "Diff Modified",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#dc7759"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": ["markup.deleted", "meta.diff.header.from-file"],
      "settings": {
        "foreground": "#dc597f"
      }
    },
    {
      "name": "JSX/TSX Text Content",
      "scope": [
        "meta.jsx.children",
        "meta.tsx.children",
        "JSXText",
        "text.html.basic",
        "meta.jsx.children.tsx",
        "meta.jsx.children.jsx"
      ],
      "settings": {
        "foreground": "#e5ded6"
      }
    }
  ]
}
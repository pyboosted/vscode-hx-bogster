    "editorSuggestWidget.background": "#232d38",
    "editorSuggestWidget.foreground": "#e5ded6",
    "editorSuggestWidget.highlightForeground": "#36b2d4",
    "editorSuggestWidget.selectedBackground": "#313f4e",
    "editorSuggestWidget.border": "#00000000",
    
    "editorHoverWidget.background": "#232d38",
    "editorHoverWidget.foreground": "#e5ded6",
